{
	"name": "codestream.intellij",
	"version": "1.0.0",
	"description": "CodeStream for IntelliJ",
	"main": "index.js",
	"author": "",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": ""
	},
	"scripts": {
		"analyze": "webpack --env.analyze",
		"build": "npm run agent:build && webpack --env.development",
		"bundle": "npm install --no-save && npm run agent:bundle && npm run agent:pkg && webpack --env.production",
		"bundle:mac": "npm run agent:bundle && npm run agent:pkg:mac && webpack --env.production",
		"bundle:ci": "npm run clean:ci && npm run bundle",
		"clean": "git clean -Xdf -e !licenses/**/* !node_modules -e !node_modules/**/* -e !codestream*.info",
		"clean:ci": "git clean -xdf -e licenses/**/* && npm run webview:clean:ci && npm run agent:clean:ci",
		"rebuild": "npm run clean && npm install --no-save && npm run agent:rebuild && npm run build",
		"watch": "webpack --watch --env.development --info-verbosity verbose",
		"agent:build": "pushd \"../shared/agent\" && npm run build && popd",
		"agent:bundle": "pushd \"../shared/agent\" && npm run bundle && popd",
		"agent:clean:ci": "pushd \"../shared/agent\" && npm run clean:ci && popd",
		"agent:pkg": "pushd \"../shared/agent\" && npm run pkg-jetbrains && popd",
		"agent:pkg:mac": "pushd \"../shared/agent\" && npm run pkg-jetbrains-mac && popd",
		"agent:rebuild": "pushd \"../shared/agent\" && npm run rebuild && popd",
		"agent:test": "pushd \"../shared/agent\" && npm run test && popd",
		"agent:watch": "pushd \"../shared/agent\" && npm run watch && popd",
		"webview:analyze": "webpack --env.analyzeWebview",
		"webview:clean:ci": "pushd \"../shared/ui\" && npm run clean:ci && popd",
		"preinstall": "pushd \"../shared/ui\" && npm install --no-save && popd"
	},
	"dependencies": {
		"core-js": "2.6.9"
	},
	"devDependencies": {
		"@babel/cli": "7.7.0",
		"@babel/core": "7.7.2",
		"@babel/plugin-proposal-class-properties": "7.7.0",
		"@babel/plugin-proposal-object-rest-spread": "7.6.2",
		"@babel/plugin-proposal-optional-chaining": "7.13.8",
		"@babel/plugin-syntax-dynamic-import": "7.2.0",
		"@babel/plugin-transform-runtime": "7.6.2",
		"@babel/preset-env": "7.7.1",
		"@babel/preset-react": "7.7.0",
		"@babel/preset-typescript": "7.7.2",
		"@babel/runtime": "7.7.2",
		"@types/keytar": "4.4.0",
		"@types/lodash-es": "4.17.3",
		"@types/node": "10.1.4",
		"@types/node-fetch": "2.3.7",
		"@types/uuid": "3.4.5",
		"babel-loader": "8.0.6",
		"circular-dependency-plugin": "5.0.2",
		"clean-webpack-plugin": "1.0.1",
		"css-loader": "6.8.1",
		"fork-ts-checker-webpack-plugin": "1.3.7",
		"html-loader": "0.5.5",
		"html-webpack-plugin": "3.2.0",
		"less": "3.9.0",
		"less-loader": "5.0.0",
		"mini-css-extract-plugin": "1.6.0",
		"prettier": "1.18.2",
		"terser-webpack-plugin": "1.4.4",
		"ts-loader": "6.0.4",
		"tsconfig-paths-webpack-plugin": "3.2.0",
		"tslint": "5.18.0",
		"tslint-loader": "3.5.4",
		"typescript": "3.6.4",
		"vsce": "1.88.0",
		"webpack": "4.46.0",
		"webpack-bundle-analyzer": "3.8.0",
		"webpack-cli": "3.3.12"
	}
}
